    default_transform: keymap_transform0 {
        compatible = "zmk,matrix-transform";
        columns = <7>;
        rows = <16>;
        map = <
        RC(0,1) RC(1,0) RC(2,0) RC(3,0) RC(4,0) RC(5,0) RC(6,0) RC(7,0)     RC(8,1) RC(9,0) RC(10,0) RC(11,0) RC(12,0) RC(13,0) RC(14,0) RC(15,0) RC(15,1)
        RC(0,2) RC(1,2) RC(2,1) RC(3,1) RC(4,1) RC(5,1) RC(6,1) RC(7,1)     RC(8,2) RC(9,2) RC(10,1) RC(11,1) RC(12,1) RC(13,1) RC(14,1)          RC(15,3)
        RC(0,3) RC(1,3)         RC(3,2) RC(4,2) RC(5,2) RC(6,2) RC(7,2)     RC(8,3) RC(9,3) RC(10,3) RC(11,2) RC(12,2) RC(13,2) RC(14,2) RC(15,2) RC(15,4)
        RC(0,4) RC(1,4)         RC(3,4) RC(4,3) RC(5,3) RC(6,3) RC(7,3)     RC(8,4) RC(9,4) RC(10,4) RC(11,4) RC(12,3) RC(13,3) RC(14,3)          RC(15,5)
        RC(0,5) RC(1,5)         RC(3,5) RC(4,5) RC(5,4) RC(6,4) RC(7,4)     RC(8,5) RC(9,5) RC(10,5) RC(11,5) RC(12,5) RC(13,4)           RC(14,4)
        RC(0,6) RC(1,6) RC(2,6) RC(3,6) RC(4,6)         RC(5,6) RC(6,5)     RC(8,6) RC(9,6)                                     RC(10,6) RC(11,6) RC(12,6)
        >;
    };

    kscan0: kscan0 {
        compatible = "zmk,kscan-gpio-charlieplex";
        wakeup-source;
    };


    left1_encoder: encoder_left1 {
        compatible = "alps,ec11";
        a-gpios = <&gpio0 9 (GPIO_ACTIVE_HIGH | GPIO_PULL_UP)>;
        b-gpios = <&gpio0 10 (GPIO_ACTIVE_HIGH | GPIO_PULL_UP)>;
        steps = <40>;
        status = "disabled";
    };
    left2_encoder: encoder_left2 {
        compatible = "alps,ec11";
        a-gpios = <&gpio0 19 (GPIO_ACTIVE_HIGH | GPIO_PULL_UP)>;
        b-gpios = <&gpio1 3 (GPIO_ACTIVE_HIGH | GPIO_PULL_UP)>;
        steps = <80>;
        status = "disabled";
    };

    right1_encoder: encoder_right1 {
        compatible = "alps,ec11";
        a-gpios = <&gpio0 9 (GPIO_ACTIVE_HIGH | GPIO_PULL_UP)>;
        b-gpios = <&gpio0 10 (GPIO_ACTIVE_HIGH | GPIO_PULL_UP)>;
        steps = <40>;
        status = "disabled";
    };
    right2_encoder: encoder_right2 {
        compatible = "alps,ec11";
        a-gpios = <&gpio0 19 (GPIO_ACTIVE_HIGH | GPIO_PULL_UP)>;
        b-gpios = <&gpio1 1 (GPIO_ACTIVE_HIGH | GPIO_PULL_UP)>;
        steps = <80>;
        status = "disabled";
    };

    sensors: sensors {
        compatible = "zmk,keymap-sensors";
        sensors = <&left1_encoder &left2_encoder &right1_encoder &right2_encoder>;
        triggers-per-rotation = <20>;
    };
};

a-gpios = <&gpio0 19 (GPIO_ACTIVE_HIGH | GPIO_PULL_UP)>;
        b-gpios = <&gpio1 1 (GPIO_ACTIVE_HIGH | GPIO_PULL_UP)>;